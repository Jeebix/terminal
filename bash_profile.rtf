\cb3 \} \cb1 \
\
\cf2 \cb3 \strokec2 #   findPid: find out the pid of a specified process\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   -----------------------------------------------------\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       Note that the command name can be specified via a regex\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       E.g. findPid '/d$/' finds pids of all processes with names ending in 'd'\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       Without the 'sudo' it will only find processes of the current user\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   -----------------------------------------------------\cf4 \cb1 \strokec4 \
\cb3     \cf9 \strokec9 findPid\cf4 \strokec4  () \{ lsof -t -c \cf6 \strokec6 "\cf4 \strokec4 $@\cf6 \strokec6 "\cf4 \strokec4  \cf5 \strokec5 ;\cf4 \strokec4  \}\cb1 \
\
\cf2 \cb3 \strokec2 #   memHogsTop, memHogsPs:  Find memory hogs\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   -----------------------------------------------------\cf4 \cb1 \strokec4 \
\cb3     \cf8 \strokec8 alias\cf4 \strokec4  memHogsTop=\cf6 \strokec6 'top -l 1 -o rsize | head -20'\cf4 \cb1 \strokec4 \
\cb3     \cf8 \strokec8 alias\cf4 \strokec4  memHogsPs=\cf6 \strokec6 'ps wwaxm -o pid,stat,vsize,rss,time,command | head -10'\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #   cpuHogs:  Find CPU hogs\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   -----------------------------------------------------\cf4 \cb1 \strokec4 \
\cb3     \cf8 \strokec8 alias\cf4 \strokec4  cpu_hogs=\cf6 \strokec6 'ps wwaxr -o pid,stat,%cpu,time,command | head -10'\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #   topForever:  Continual 'top' listing (every 10 seconds)\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   -----------------------------------------------------\cf4 \cb1 \strokec4 \
\cb3     \cf8 \strokec8 alias\cf4 \strokec4  topForever=\cf6 \strokec6 'top -l 9999999 -s 10 -o cpu'\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #   ttop:  Recommended 'top' invocation to minimize resources\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   ------------------------------------------------------------\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       Taken from this macosxhints article\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #       http://www.macosxhints.com/article.php?story=20060816123853639\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   ------------------------------------------------------------\cf4 \cb1 \strokec4 \
\cb3     \cf8 \strokec8 alias\cf4 \strokec4  ttop=\cf6 \strokec6 "top -R -F -s 10 -o rsize"\cf4 \cb1 \strokec4 \
\
\cf2 \cb3 \strokec2 #   my_ps: List processes owned by my user:\cf4 \cb1 \strokec4 \
\cf2 \cb3 \strokec2 #   ------------------------------------------------------------\cf4 \cb1 \strokec4 \
\cb3     \cf9 \strokec9 my_ps\cf4 \strokec4 () \{ ps $@ -u $USER -o pid,%cpu,%mem,start,time,bsdtime,command \cf5 \strokec5 ;\cf4 \strokec4  \}\cb1 \
